{
  "name": "game-editor",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./server/cmd/run.mjs",
    "start:old": "concurrently \"ng serve\" \"npm run build:electron\" \"npm run start:electron\"",
    "build:electron": "npx tsc -p ./server/tsconfig.json",
    "start:electron": "wait-on \"http://localhost:4200\" && electron ./dist/index.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "tailwind:update": "npx tailwindcss -c ./tailwind.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
